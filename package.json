{
  "name": "js-forms",
  "version": "1.0.3",
  "description": "Some helpers for working with forms in Javascript",
  "main": "lib/index.js",
  "scripts": {
    "clean:docs": "rimraf docs && mkdir docs",
    "clean:lib": "rimraf lib && mkdir lib",
    "clean:dist": "rimraf dist && mkdir dist",
    "clean": "npm run clean:lib && npm run clean:dist",
    "lib": "babel src -d lib",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ]
    ],
    "comments": false,
    "sourceMaps": "inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beforan/js-forms.git"
  },
  "author": "beforan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beforan/js-forms/issues"
  },
  "homepage": "https://github.com/beforan/js-forms#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "babelify": "^8.0.0",
    "browserify": "^15.2.0",
    "gulp": "^3.9.1",
    "rimraf": "^2.6.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "formdata-polyfill": "^3.0.9"
  }
